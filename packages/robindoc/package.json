{
  "name": "robindoc",
  "version": "3.2.1",
  "description": "Robindoc is a framework for automatically creating documentation websites based on markdown files, built on React.js Server Components",
  "main": "./lib/index.js",
  "scripts": {
    "prepare": "ts-patch install -s",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "files": [
    "lib",
    "assets"
  ],
  "keywords": [
    "documentation",
    "generation",
    "generate",
    "auto",
    "SSG",
    "next.js",
    "RSC",
    "docs",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vordgi/robindoc.git"
  },
  "author": {
    "name": "Savelyev Alexander",
    "email": "vordgi@mail.com",
    "url": "https://github.com/vordgi/"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "2.1.1",
    "dot-prop": "10.1.0",
    "github-slugger": "2.0.0",
    "glob": "13.0.0",
    "gray-matter": "4.0.3",
    "html-react-parser": "5.2.11",
    "marked": "17.0.1",
    "shiki": "3.20.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/github-slugger": "2.0.0",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rollup": "4.54.0",
    "rollup-plugin-scss": "4.0.1",
    "rollup-preserve-directives": "1.1.3",
    "sass": "1.97.1",
    "ts-patch": "3.3.0",
    "typescript": "5.9.3",
    "typescript-transform-paths": "3.5.5"
  },
  "peerDependencies": {
    "next": ">= 13.0.0",
    "react": ">= 18.3.0",
    "react-dom": ">= 18.3.0"
  }
}